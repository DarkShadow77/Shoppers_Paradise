<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppersparadise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShoppersParadise">
        <activity android:name=".Order"></activity>
        <activity android:name=".Profile" />
        <activity android:name=".OrderHistory" />
        <activity android:name=".ProifileSetting" />
        <activity android:name=".OrderDetails" />
        <activity android:name=".ContactUs" />
        <activity android:name=".CategoriesPage" />
        <activity android:name=".FeedbackPage" />
        <activity android:name=".Product_Page" />
        <activity android:name=".Home" />
        <activity android:name=".SignUp" />
        <activity android:name=".SignIn" />
        <activity android:name=".After_splash" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>